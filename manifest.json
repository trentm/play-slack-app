{
    "display_information": {
        "name": "play-slack-app",
        "description": "Trent's play slack app for OnWeek 2024-12",
        "background_color": "#2c2d30"
    },
    "features": {
        "bot_user": {
            "display_name": "play-slack-app",
            "always_online": false
        }
    },
    "oauth_config": {
        "scopes": {
            "bot": [
                "chat:write",
                "channels:history",
                "groups:history"
            ]
        }
    },
    "settings": {
        "event_subscriptions": {
            "bot_events": [
                "message.channels",
                "message.groups"
            ]
        },
        "interactivity": {
            "is_enabled": true
        },
        "org_deploy_enabled": false,
        "socket_mode_enabled": true,
        "token_rotation_enabled": false
    }
}
